    <script type="text/javascript">
        // --- MASTER JSON DATABASE (Simulated from user input) ---
        const MASTER_DB = JSON.parse(document.getElementById('master-db-json').textContent);

        // Simulated Global State (like a Firebase DB structure)
        const productsData = MASTER_DB.PRODUCTS;
        const ecomCategories = MASTER_DB.E_COMMERCE_CATEGORIES;
        const attributes = MASTER_DB.PRODUCT_ATTRIBUTES;
        const notificationsHistory = MASTER_DB.NOTIFICATIONS.history;
        const reviewsData = MASTER_DB.RATINGS_REVIEWS;
        
        // Simulating user data
        const currentUser = { uid: 'user_uid_12345' };
        let cart = {}; // { 'prod_id_T001_RED_M': 1, 'prod_id_T002_BLACK_OS': 2 }
        let wishlist = {};
        let goldBalance = 0.0000;
        let walletBalance = 0.00;
        let selectedVariantKey = null; // Used for product modal

        let modalProductId = null; // Currently open product in modal

        // Helper function for simplified Firebase simulation
        const db = {
            ref: (path) => ({
                set: (value) => {
                    console.log(`DB WRITE: ${path} =`, value);
                    // Update local state for cart/wishlist
                    if (path.startsWith('users/' + currentUser.uid + '/cart')) {
                        cart = value;
                        renderCart();
                    }
                    if (path.startsWith('users/' + currentUser.uid + '/wishlist')) {
                        wishlist = value;
                    }
                },
                once: () => Promise.resolve({ val: () => ({ /* simplified data retrieval */ }) }),
                on: (event, callback) => {
                    // Simplified listener for admin toggle on services
                    if (path.startsWith('services/service')) {
                         // Simulate all e-commerce services as 'on'
                        const serviceId = path.split('/').pop();
                        if (serviceId.startsWith('service')) {
                            callback('on'); // Assume all exist to honor "don't hide anything"
                        }
                    }
                }
            })
        };

        // --- CORE UTILITIES ---
        function renderStatusBadge(status) {
            let badgeClass = '';
            switch (status) {
                case 'Pending': badgeClass = 'badge-pending'; break;
                case 'Shipped':
                case 'In_Transit': badgeClass = 'badge-processing'; break;
                case 'Delivered': badgeClass = 'badge-delivered'; break;
                default: badgeClass = 'badge-pending';
            }
            return `<span class="badge ${badgeClass}">${status.replace(/_/g, ' ')}</span>`;
        }

        function toggleTheme(){
            const currentTheme = document.body.getAttribute('data-theme');
            const newTheme = currentTheme === 'light' ? 'dark' : 'light';
            document.body.setAttribute('data-theme', newTheme);
            document.getElementById('theme-icon').className = newTheme === 'light' ? 'ri-moon-line' : 'ri-sun-line';
        }
        
        function navClick(e){
            document.querySelectorAll('.navbtn').forEach(n=>n.classList.remove('active'));
            e.currentTarget.classList.add('active');
            showTab(e.currentTarget.dataset.target);
        }
        
        function showTab(id){
            document.querySelectorAll('.section').forEach(s=>s.classList.remove('active'));
            const el = document.getElementById(id);
            if(el) el.classList.add('active');
            closeOverlay(); 
            // Load specific content when tab is shown
            if (id === 'shop') showShopView('categories');
            if (id === 'home') updateLatestPreview();
            if (id === 'notifications') loadNotifications();
        }
        
        function closeOverlay() {
            document.getElementById('product-modal-overlay').style.display = 'none';
            selectedVariantKey = null; // Clear selected variant on close
        }

        // --- NOTIFICATIONS LOGIC (User Request: uper aana cahiye, admin भेजो to bell pr, new product ad ka bell pr) ---
        function loadNotifications() {
            const list = document.getElementById('notifications-list');
            list.innerHTML = '';
            
            // Convert history object to array for display
            const notifArray = Object.keys(notificationsHistory).map(key => notificationsHistory[key]).reverse();
            
            notifArray.forEach(n => {
                const card = document.createElement('div');
                card.className = 'card';
                card.style.borderLeft = n.user_id === 'all' ? '4px solid var(--gold-strong)' : '4px solid #6366f1';
                card.innerHTML = `
                    <div style="font-weight:600;">${n.title}</div>
                    <div class="small-muted">${n.message} (${n.timestamp ? n.timestamp.substring(0, 10) : 'n/a'})</div>
                `;
                list.appendChild(card);
            });
            // Reset badge after viewing
            document.getElementById('notification-badge').style.display = 'none';
        }

        // Simulate a new notification from the admin
        function simulateNewAdminNotification(title, message) {
            const newId = 'notif_' + Date.now();
            notificationsHistory[newId] = {
                title: title,
                message: message,
                user_id: 'admin',
                timestamp: new Date().toISOString()
            };
            // Show badge on bell icon
            document.getElementById('notification-badge').style.display = 'block';
        }


        // --- E-COMMERCE LOGIC (User Request: all category aani cahiye, color/size selection) ---
        
        // Maps e-commerce category keys to an icon
        const ECOM_ICONS = {
            "Fashion_Apparel": "ri-t-shirt-line", "Electronics_Gadgets": "ri-headphone-line",
            "Home_Kitchen": "ri-home-line", "Beauty_Care": "ri-lipstick-line"
        };
        const ECOM_IMAGES = {
             "Fashion_Apparel": "https://via.placeholder.com/55/d4af37/ffffff?text=F", "Electronics_Gadgets": "https://via.placeholder.com/55/3b82f6/ffffff?text=E",
            "Home_Kitchen": "https://via.placeholder.com/55/10b981/ffffff?text=H", "Beauty_Care": "https://via.placeholder.com/55/f97316/ffffff?text=B"
        };


        function populateECommerceCategories(containerId, isFullGrid = false) {
            const container = document.getElementById(containerId);
            container.innerHTML = '';
            const mainCategories = ecomCategories;

            Object.keys(mainCategories).forEach(catKey => {
                const catName = catKey.replace(/_/g, ' ');
                const card = document.createElement('div');
                card.className = 'category-card';
                card.setAttribute('onclick', `showProductsByCategory('${catKey}')`);
                
                const iconOrImage = ECOM_IMAGES[catKey] 
                    ? `<img src="${ECOM_IMAGES[catKey]}" alt="${catName}">`
                    : `<i class="${ECOM_ICONS[catKey] || 'ri-apps-line'}"></i>`;

                card.innerHTML = `
                    <div class="category-icon">${iconOrImage}</div>
                    <div class="category-name">${catName}</div>
                `;
                container.appendChild(card);
            });
        }
        
        function showShopView(view, categoryKey = null) {
            const categoryView = document.getElementById('shop-category-view');
            const productListView = document.getElementById('shop-product-list-view');
            const backBtn = document.getElementById('back-to-categories');
            
            if (view === 'categories') {
                categoryView.classList.remove('field-hidden');
                productListView.classList.add('field-hidden');
                backBtn.style.display = 'none';
                document.getElementById('shop-title').innerText = 'E-Commerce Shop';
                populateECommerceCategories('shop-all-categories-grid');
            } else if (view === 'products' && categoryKey) {
                categoryView.classList.add('field-hidden');
                productListView.classList.remove('field-hidden');
                backBtn.style.display = 'inline-block';
                const title = categoryKey.replace(/_/g, ' ');
                document.getElementById('shop-title').innerText = title;
                document.getElementById('product-list-title').innerText = `Products in ${title}`;
                renderProducts('products-list-container', categoryKey);
            }
        }

        function showProductsByCategory(categoryKey) {
            showShopView('products', categoryKey);
        }


        function renderProducts(containerId, categoryFilter = null) {
            const cont = document.getElementById(containerId);
            cont.innerHTML = '';
            let productKeys = Object.keys(productsData);

            if (categoryFilter) {
                productKeys = productKeys.filter(k => productsData[k].category === categoryFilter);
            }
            
            // For home page latest preview, show only first 4
            if (containerId === 'latest-products') {
                productKeys = productKeys.slice(0, 4);
            }

            productKeys.forEach(k => {
                const p = productsData[k];
                const firstVariantKey = Object.keys(p.variants)[0];
                const firstVariant = p.variants[firstVariantKey];
                const displayPrice = firstVariant.price;

                const card = document.createElement('div');
                card.className = 'card product-card-inner';
                card.setAttribute('onclick', `openProductModal('${k}')`);
                
                const reviewCount = (reviewsData[k] ? Object.keys(reviewsData[k]).length : 0);

                card.innerHTML = `
                    <img src="${p.images[0] || 'https://via.placeholder.com/150'}" class="product-img" alt="${p.name}">
                    <span class="pmeta pname">${p.name}</span>
                    <span class="pmeta pprice">₹${displayPrice}</span>
                    <div class="small-muted">${p.category.replace(/_/g, ' ')}</div>
                    <div class="product-actions">
                        <span class="rating">
                            ${'★'.repeat(Math.round(p.rating))}
                            ${'☆'.repeat(5 - Math.round(p.rating))}
                        </span>
                        <span class="small-muted">(${reviewCount})</span>
                        <button class="btn secondary" onclick="toggleWishlist(event, '${k}')" title="Wishlist">
                            ${wishlist[k] ? `<i class="ri-heart-fill" style="color:red;"></i>` : `<i class="ri-heart-line"></i>`}
                        </button>
                    </div>
                `;
                cont.appendChild(card);
            });
        }
        
        // --- PRODUCT MODAL & VARIANT LOGIC ---
        
        function getVariantKey(color, size) {
            // Converts "Red" and "M" to "RED_M" to match JSON keys
            return (color || 'OS').toUpperCase().replace(/\s/g, '_') + '_' + (size || 'OS').toUpperCase().replace(/\s/g, '_');
        }

        function handleVariantSelection(type, value) {
            const p = productsData[modalProductId];
            if (!p) return;

            let currentColor = document.querySelector('#modal-color-options .variant-option.selected')?.dataset.value;
            let currentSize = document.querySelector('#modal-size-options .variant-option.selected')?.dataset.value;

            if (type === 'color') {
                currentColor = value;
            } else if (type === 'size') {
                currentSize = value;
            }
            
            // Clear previous selections for the given type
            document.querySelectorAll(`#modal-${type}-options .variant-option`).forEach(opt => {
                opt.classList.remove('selected');
                if (opt.dataset.value === value) {
                    opt.classList.add('selected');
                }
            });

            // Find matching variant key
            selectedVariantKey = Object.keys(p.variants).find(key => {
                const v = p.variants[key];
                const colorMatch = currentColor ? v.color === currentColor : true;
                const sizeMatch = currentSize ? v.size === currentSize : true;
                return colorMatch && sizeMatch;
            });

            // If a variant is selected, update price/stock
            const stockInfo = document.getElementById('modal-stock-info');
            const addToCartBtn = document.getElementById('modal-add-to-cart');
            const priceDisplay = document.getElementById('modal-price');

            if (selectedVariantKey && p.variants[selectedVariantKey]) {
                const variant = p.variants[selectedVariantKey];
                priceDisplay.innerText = `₹${variant.price}`;
                stockInfo.style.color = variant.stock > 10 ? 'green' : (variant.stock > 0 ? 'orange' : 'red');
                stockInfo.innerText = `Stock: ${variant.stock > 0 ? variant.stock : 'Out of Stock'}`;
                addToCartBtn.disabled = variant.stock <= 0;
            } else {
                priceDisplay.innerText = `₹${p.base_price}`;
                stockInfo.style.color = 'red';
                stockInfo.innerText = 'Please select a valid variant.';
                addToCartBtn.disabled = true;
            }
        }
        
        function openProductModal(id){
            const p = productsData[id];
            if(!p) return alert('product not found');
            modalProductId = id;
            selectedVariantKey = null; // Reset selection

            document.getElementById('modal-title').innerText = p.name;
            document.getElementById('modal-price').innerText = `₹${p.base_price}`;
            document.getElementById('modal-desc').innerText = p.description || '';

            // Image Rendering
            const imgs = document.getElementById('modal-images');
            imgs.innerHTML=''; 
            (p.images||[]).forEach(url=>{
                const img = document.createElement('img');
                img.src = url;
                imgs.appendChild(img);
            });

            // Reviews/Rating
            const rating = p.rating || 0;
            const reviewCount = reviewsData[id] ? Object.keys(reviewsData[id]).length : 0;
            document.getElementById('modal-rating').innerHTML = `
                ${'★'.repeat(Math.round(rating))}
                ${'☆'.repeat(5 - Math.round(rating))} 
                &nbsp;(${reviewCount})
            `;
            const reviewsList = document.getElementById('modal-reviews-list');
            reviewsList.innerHTML = '';
            if (reviewsData[id]) {
                Object.values(reviewsData[id]).forEach(review => {
                    reviewsList.innerHTML += `<div class="card small" style="margin-bottom:8px; padding:8px;">
                        <div class="rating" style="margin-bottom:4px;">${'★'.repeat(review.rating)}${'☆'.repeat(5 - review.rating)}</div>
                        <div>${review.review_text}</div>
                        <div class="small-muted" style="margin-top:4px;">- User ID: ${review.user_id}</div>
                    </div>`;
                });
            } else {
                reviewsList.innerHTML = '<div class="small-muted">No reviews yet. Be the first!</div>';
            }


            // Variant Rendering (Color & Size)
            const availableColors = new Set();
            const availableSizes = new Set();
            Object.values(p.variants).forEach(v => {
                if (v.color) availableColors.add(v.color);
                if (v.size && v.size !== 'OS') availableSizes.add(v.size);
            });

            const colorOptionsDiv = document.getElementById('modal-color-options');
            const sizeOptionsDiv = document.getElementById('modal-size-options');
            colorOptionsDiv.innerHTML = '';
            sizeOptionsDiv.innerHTML = '';
            
            document.getElementById('modal-variants-section').style.display = (availableColors.size > 0 || availableSizes.size > 0) ? 'block' : 'none';


            availableColors.forEach(color => {
                colorOptionsDiv.innerHTML += `<div class="variant-option" data-value="${color}" onclick="handleVariantSelection('color', '${color}')">${color}</div>`;
            });

            const allApparelSizes = Object.keys(attributes.Size_List_Apparel);
            allApparelSizes.filter(s => availableSizes.has(s)).forEach(size => {
                 sizeOptionsDiv.innerHTML += `<div class="variant-option" data-value="${size}" onclick="handleVariantSelection('size', '${size}')">${size}</div>`;
            });

            // Initial state (force selection or default to first variant if one exists)
            const firstVariantKey = Object.keys(p.variants)[0];
            if (firstVariantKey) {
                const v = p.variants[firstVariantKey];
                if (v.color) {
                    handleVariantSelection('color', v.color);
                }
                if (v.size && v.size !== 'OS') {
                    handleVariantSelection('size', v.size);
                } else {
                    // Force the stock/price update for single-variant products
                    selectedVariantKey = firstVariantKey;
                    handleVariantSelection('color', null); // Trigger update
                }
            } else {
                // No variants defined, default behavior
                 document.getElementById('modal-stock-info').style.color = 'green';
                 document.getElementById('modal-stock-info').innerText = 'Stock: In Stock';
                 document.getElementById('modal-add-to-cart').disabled = false;
            }
            
            document.getElementById('product-modal-overlay').style.display = 'flex';
        }

        function submitReview(productId) {
            const rating = parseInt(document.getElementById('review-rating').value);
            const reviewText = document.getElementById('review-text').value.trim();

            if (reviewText.length < 10) {
                return alert('Please write a detailed review (min 10 characters).');
            }

            const reviewId = 'review_' + Date.now();
            const newReview = {
                user_id: currentUser.uid,
                rating: rating,
                review_text: reviewText,
                item_type: 'product'
            };

            // Simulate DB write
            if (!reviewsData[productId]) {
                reviewsData[productId] = {};
            }
            reviewsData[productId][reviewId] = newReview;
            
            alert('Review submitted successfully! Pending moderation.');
            document.getElementById('review-text').value = '';
            openProductModal(productId); // Reload modal to show new review
        }
        
        // --- CART LOGIC (Updated to handle variants) ---
        function addToCart(pid, isModal) {
            let key = pid;
            
            if (isModal) {
                if (!selectedVariantKey) {
                    return alert('Please select a color and size first.');
                }
                key = pid + '_' + selectedVariantKey;
            }

            cart[key] = (cart[key] || 0) + 1;
            saveCartToDb();
            
            if(isModal) closeOverlay();
        }

        function saveCartToDb(){
            db.ref('users/'+currentUser.uid+'/cart').set(cart);
            updateCartCount();
        }

        function updateCartCount(){
            const totalCount = Object.values(cart).reduce((sum, qty) => sum + qty, 0);
            document.getElementById('cart-count').innerText = totalCount;
        }

        function renderCart() {
            const cont = document.getElementById('cart-list-container');
            if (!cont) return;
            cont.innerHTML = '';
            let subtotal = 0;

            Object.keys(cart).forEach(variantKey => {
                const qty = cart[variantKey];
                const parts = variantKey.split('_');
                const pid = parts[0];
                const product = productsData[pid];

                if (!product) return;
                
                // Find the specific variant
                let variant = null;
                const variantId = parts.slice(1).join('_');
                if (product.variants && product.variants[variantId]) {
                    variant = product.variants[variantId];
                } else if (product.variants && Object.keys(product.variants).length === 1) {
                     // For single-variant products that might not have a full key
                    variant = Object.values(product.variants)[0];
                }

                if (!variant) return;

                const price = variant.price || product.base_price;
                const totalVariantPrice = price * qty;
                subtotal += totalVariantPrice;
                
                const variantDetails = (variant.color ? variant.color : '') + 
                                       (variant.color && variant.size ? ' / ' : '') + 
                                       (variant.size && variant.size !== 'OS' ? variant.size : '');
                
                const variantText = variantDetails ? `<div class="small-muted">${variantDetails}</div>` : '';

                const div = document.createElement('div');
                div.className = 'card';
                div.innerHTML = `
                    <img src="${product.images[0] || 'https://via.placeholder.com/72'}" alt="${product.name}">
                    <div style="flex-grow:1;">
                        <div style="font-weight:600;">${product.name}</div>
                        ${variantText}
                        <div class="small-muted">Price: ₹${price} x ${qty}</div>
                        <div style="font-weight:700;">Total: ₹${totalVariantPrice}</div>
                    </div>
                    <div class="row" style="align-items:center;">
                        <button class="btn secondary" onclick="decrement('${variantKey}')" style="padding:6px 10px;">-</button>
                        <button class="btn" onclick="increment('${variantKey}')" style="padding:6px 10px;">+</button>
                        <button class="btn secondary" onclick="removeFromCart('${variantKey}')" style="padding:6px 10px;margin-left:auto;"><i class="ri-delete-bin-line"></i></button>
                    </div>
                `;
                cont.appendChild(div);
            });

            document.getElementById('cart-subtotal').innerText = subtotal.toFixed(2);
            updateCartCount();
        }

        function increment(variantKey){
            cart[variantKey] = (cart[variantKey]||0)+1;
            saveCartToDb(); 
            renderCart();
        }

        function decrement(variantKey){
            if(!cart[variantKey]) return;
            cart[variantKey] = cart[variantKey]-1;
            if(cart[variantKey]<=0) delete cart[variantKey];
            saveCartToDb(); 
            renderCart();
        }
        
        function removeFromCart(variantKey){
            delete cart[variantKey];
            saveCartToDb();
            renderCart();
        }
        
        // --- INITIALIZATION ---
        function initApp() {
            console.log('App Initializing...');
            // Load and render E-commerce categories on Home and Shop tabs
            populateECommerceCategories('ecommerce-categories-grid');
            populateECommerceCategories('shop-all-categories-grid');
            
            // Load latest products on home
            renderProducts('latest-products');
            
            // Render cart state (even if empty)
            renderCart(); 
            
            // Simulate a new product being added and trigger admin notification
            simulateNewAdminNotification("New Product Alert!", "A new product 'Noise Cancelling Headphones' has been added to the Electronics category.");
            
            // Initial view
            showTab('home');

            // Set current user name
            document.querySelector('.gold-box h3').innerText = `Welcome Back, ${MASTER_DB.USERS[currentUser.uid].name.split(' ')[0]}!`;
        }

        // Dummy/Placeholder functions for existing features
        function loadProducts() { /* Called by renderProducts */ }
        function saveWishlistToDb() { /* Simplified */ console.log('Wishlist saved:', wishlist); }
        function toggleWishlist(e, pid) { e.stopPropagation(); wishlist[pid] = !wishlist[pid]; saveWishlistToDb(); renderProducts('latest-products'); }
        function loadWishlist() { /* Renders wishlist products */ }
        function calculateGoldGrams() { /* Gold calculation logic */ }
        function submitGoldPurchase() { alert('Gold purchase request submitted. Pending UTR verification.'); }
        function submitReview(id) { alert('Review submitted. Needs integration.'); }

        // Start the application
        window.onload = initApp;

        // Ensure existing service buttons are visible (simulating Firebase 'on' status from the original snippet)
        for (let i = 1; i <= 20; i++) {
             const btn = document.getElementById("service" + i);
             if (btn) btn.style.display = 'inline-block';
        }
    </script>
    
